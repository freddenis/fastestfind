- names:
start: date
delete1: /usr/bin/time --format="\telapsed %E" find ~/swid/tmpfiles -type f -delete
delete2: time find ~/swid/tmpfiles -type f -exec rm -fr {} ;
delete3: time find ~/swid/tmpfiles -type f | xargs rm -fr
delete4: time rm -fr ~/swid/tmpfiles
deltmpfiles0: rm -fr ~/swid/tmpfiles
deltmpfiles1: rm -fr ~/swid/tmpfiles
deltmpfiles2: rm -fr ~/swid/tmpfiles
deltmpfiles3: rm -fr ~/swid/tmpfiles
cre_files1: ~/swid/cre_files.sh -n 1000 -d tmpfiles
cre_files2: ~/swid/cre_files.sh -n 1000 -d tmpfiles
cre_files3: ~/swid/cre_files.sh -n 1000 -d tmpfiles
cre_files4: ~/swid/cre_files.sh -n 1000 -d tmpfiles
end: date

- dependencies:
start:
deltmpfiles0:start
cre_files1:deltmpfiles0
delete1:cre_files1
end:delete1
